{
  "name": "publiz",
  "devDependencies": {
    "prettier": "^3.1.0",
    "turbo": "latest"
  },
  "private": true,
  "scripts": {
    "build:types": "turbo build",
    "dev": "bun run --cwd apps/api dev",
    "migrate:dev:latest": "bun run --cwd apps/api migrate:dev:latest",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
